{
  "manifest_version": 2,

  "name": "novel",
  "description": "无敌插件",
  "version": "1.0",
  "icons": {
    "32": "icon32.png",
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon64.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://cdn.bootcss.com/"
  ]
}
